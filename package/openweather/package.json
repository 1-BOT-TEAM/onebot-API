{
  "name": "@oneboteam/openweather",
  "version": "2.1.0",
  "description": "Simple api for Weather",
  "scripts": {
    "build": "tsc && npm run esm:default",
    "esm:default": "gen-esm-wrapper dist/index.js dist/index.mjs",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1-BOT-TEAM/onebot-API.git"
  },
  "module": "dist/index.mjs",
  "keywords": [
    "api"
  ],
  "files": [
    "dist/**/*"
  ],
  "author": "1-BOT TEAM",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/1-BOT-TEAM/onebot-API/issues"
  },
  "homepage": "https://github.com/1-BOT-TEAM/onebot-API#readme",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "gen-esm-wrapper": "^1.1.3",
    "prettier": "^2.8.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  }
}
