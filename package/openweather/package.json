{
  "name": "@onebot/openweather",
  "version": "1.0.0",
  "description": "Simple api for Weather",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1-BOT-TEAM/onebot-API.git"
  },
  "keywords": [
    "api"
  ],
  "files": [
    "dist/**/*"
  ],
  "author": "1-BOT TEAM",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/1-BOT-TEAM/onebot-API/issues"
  },
  "homepage": "https://github.com/1-BOT-TEAM/onebot-API#readme",
  "devDependencies": {
    "prettier": "^2.8.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "node-fetch": "^3.3.0"
  },
  "type": "module"
}